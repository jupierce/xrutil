{
  "kind" : "ObjectRepository",
  "metadata" : {
    "name" : "to_qe"
  },

  "spec" : {
    "type" : "git",

    "git" : {
      "format" : "json",
      "uri" : "git@github.com:jupierce/xrtest.git",
      "branch" : {
        "prefix" : "ruby-hello-world_"
      }
    },

    "exportRules" : {
      "include" : "dc, svc/ruby-hello-world, secrets/test-secret",
      "exclude" : "dc/doesntexist",
      "transforms" : {
        "patches" : [
          {
            "match": "deploymentconfigs",
            "patch" : ".spec.strategy.type=\"Custom\"",
            "type" : "jq"
          },
          {
            "match": "deploymentconfigs",
            "patch" : ".spec.strategy.type=\"Rolling\"",
            "type" : "jq"
          }
        ],
        "imageMappings" : [
          {
            "pattern": "*/*/*:*",
            "setRegistryHost" : "docker.io",
            "setNamespace" : "jupierce",
            "tagType" : "generated"
          }
        ]
      }
    },

    "importRules" : {
      "include" : "dc, svc, configmaps, secrets",
      "exclude" : "",
      "namespace" : "",
      "transforms" : {
        "addNamePrefix" : "",
        "patches" : [
          {
            "match": "deploymentconfigs",
            "patch" : ".spec.strategy.type=\"Custom\"",
            "type" : "jq"
          },
          {
            "match": "deploymentconfigs",
            "patch" : ".spec.strategy.type=\"Rolling\"",
            "type" : "jq"
          }
        ],
        "imageMappings" : [
          {
            "pattern": "~/*/*:*",
            "setRegistryHost" : "docker.io",
            "setNamespace" : "jupierce"
          }
        ]
      }
    }


  }
}
